<div class="row">
   <div class="col-md-8">
     <h2 class="text-center" style="margin: 20px;"><u>My wikis</u></h2>
     <% @wikis.each do |wiki| %>
       <div class="media" style="margin: 40px;">
         <div class="media-body">
           <h4 class="media-heading text-center">
             <%= link_to wiki.title, wiki %>
           </h4>
         </div>
       </div>
     <% end %>
   </div>
   <div class="col-md-4">
     <div style="margin: 20px;">
      <%= link_to "New Wiki", new_wiki_path, class: 'btn btn-success' %>
      </div>
    </div>
</div>
